<Page x:Class="JumpBoard.Home"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:JumpBoard"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Example">

    <Grid>
        <ListView x:Name="SoundboardsListView" SelectionChanged="{Binding SoundboardListView_SelectionChanged}" Margin="0,0,555,0">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        
        <Frame x:Name="SoundboardFrame" Margin="245,0,0,0"/>
    </Grid>
</Page>
